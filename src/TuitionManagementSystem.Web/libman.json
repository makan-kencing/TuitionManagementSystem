{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "defaultDestination": "wwwroot/lib",
  "libraries": [
    {
      "library": "bootstrap@latest",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.rtl.css",
        "dist/css/bootstrap-grid.css",
        "dist/css/bootstrap-grid.rtl.css",
        "dist/css/bootstrap-reboot.css",
        "dist/css/bootstrap-reboot.rtl.css",
        "dist/css/bootstrap-utilities.css",
        "dist/css/bootstrap-utilities.rtl.css",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.esm.js",
        "dist/js/bootstrap.esm.min.js",
        "LICENSE"
      ]
    },
    {
      "library": "jquery@latest",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js",
        "dist/jquery.slim.js",
        "dist/jquery.slim.min.js",
        "LICENSE.txt"
      ]
    },
    {
      "library": "jquery-ajax-unobtrusive@latest",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive",
      "files": [
        "dist/jquery.unobtrusive-ajax.js",
        "dist/jquery.unobtrusive-ajax.min.js",
        "LICENSE.txt"
      ]
    },
    {
      "library": "jquery-validation@latest",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js",
        "dist/additional-methods.js",
        "dist/additional-methods.min.js",
        "LICENSE.md"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@latest",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js",
        "LICENSE.txt"
      ]
    },
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.js"
      ]
    }
  ]
}
